               <image>
                  <Image url="@../png_icons/rings_icon_icon.png" />
               </image>
            </ImageView>
         </graphic>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </Button>
      <Button mnemonicParsing="false" onAction="#onLogoutButtonClick" style="-fx-background-color: &quot;white&quot;; -fx-background-radius: 0;">
