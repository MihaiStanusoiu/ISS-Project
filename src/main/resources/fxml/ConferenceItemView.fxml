<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControllerConferenceItem">
   <center>
      <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" style="-fx-background-color: linear-gradient(to bottom left, #4DE0EB 0%, #6A75D9 100%);">
         <graphic>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <children>
                  <Label fx:id="date" text="Date" textFill="WHITE">
                     <font>
                        <Font name="Proxima Nova Regular" size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="location" text="Location" textFill="WHITE">
                     <font>
                        <Font name="Proxima Nova Regular" size="16.0" />
                     </font>
                  </Label>
                  <Pane VBox.vgrow="ALWAYS" />
                  <Label fx:id="acronym" alignment="BOTTOM_LEFT" contentDisplay="CENTER" opacity="0.4" text="ACRON">
                     <font>
                        <Font name="Proxima Nova Bold" size="30.0" />
                     </font>
                     <effect>
                        <Blend mode="OVERLAY" />
                     </effect>
                  </Label>
                  <Label fx:id="name" alignment="BOTTOM_LEFT" text="Name" textFill="WHITE">
                     <font>
                        <Font name="Proxima Nova Regular" size="16.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </graphic>
      </Button>
   </center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
